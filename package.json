{
  "name": "sheira-frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "turbo": "next dev -p 3000 --turbo",
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "start:dev": "next start -p 3000",
    "lint": "next lint --no-cache",
